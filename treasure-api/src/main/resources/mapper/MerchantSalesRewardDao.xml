<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.treasure.dao.MerchantSalesRewardDao">

    <select id="selectByMerchantTypeId" resultType="io.treasure.entity.MerchantSalesRewardEntity">

        select * from merchant_sales_reward
        where merchant_type = #{commissionType}

    </select>
</mapper>
